AT_CHECK([cp $srcdir/matlab/test_write_2d_numeric.m .
          $MATLABEXE -nosplash -nojvm -r "type='double';test_write_2d_numeric" | $GREP PASSED],[0],[PASSED
],[])
AT_CLEANUP

